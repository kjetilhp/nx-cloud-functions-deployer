{
	"root": "apps/functions",
	"sourceRoot": "apps/functions/src",
	"projectType": "application",
	"targets": {
		"deploy": {
			"executor": "../dist:deploy",
			"options": {
				"flavors": {
					"dev": "localekey-dev",
					"prod": "localekey-dev"
				}
			}
		},
		"build": {
			"executor": "../dist:build",
			"options": {
				"input": "src/controllers/api/test_api.ts",
				"output": "dist/controllers/api/test_api.ts"
			}
		},
		"script": {
			"executor": "../dist:script",
			"options": {
				"flavors": {
					"dev": "localekey-dev",
					"prod": "localekey-dev"
				}
			}
		}
	}
}
