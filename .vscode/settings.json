{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"eslint.validate": ["typescript"],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"files.maxMemoryForLargeFilesMB": 6144,
	"cSpell.words": [
		"buildable",
		"devkit",
		"Esbuild",
		"execa",
		"firestore",
		"JSONBIN",
		"metafile",
		"nanospinner",
		"nrwl",
		"nvexeca",
		"opentelemetry",
		"outfile",
		"preinstall",
		"rawlist"
	],
	"[dart]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": "explicit"
		}
	}
}
